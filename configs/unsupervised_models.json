{
    "FastICA": {
        "module": "sklearn.decomposition",
        "class": "FastICA",
        "normalization": "standard",
        "total_components": 100,
        "components_for_reconstruction": [1, 10, 30, 50, 100],
        "params": {
            "algorithm": "parallel",
            "fun": "logcosh",
            "max_iter": 500,
            "tol": 0.0001,
            "random_state": 42
        }
    },
    "NMF": {
        "module": "sklearn.decomposition",
        "class": "NMF",
        "normalization": "none",
        "total_components": 100,
        "components_for_reconstruction": [1, 10, 30, 50, 100],
        "params": {
            "init": "random",
            "solver": "mu",
            "beta_loss": "frobenius",
            "max_iter": 5000,
            "tol": 0.00001,
            "random_state": 42
        }
    },
    "PCA": {
        "module": "sklearn.decomposition",
        "class": "PCA",
        "normalization": "standard",
        "total_components": 100,
        "components_for_reconstruction": [1, 10, 30, 50, 100],
        "params": {
            "svd_solver": "randomized",
            "whiten": true,
            "random_state": 42
        }
    }
}
